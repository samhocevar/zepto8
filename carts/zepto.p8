pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- zepto8
-- by rez

function _init()
	cursor(0,0)
	debug=true
	pi=3.14159265358979
	ar=pi/180
	a=45   --main angle
	t=0				--timer
	r=128  --focale
	s=2	   --speed
	m=16   --margin
	str={} --stars
	for i=0,255 do
		str[i]={
			x=rnd(256)-128,
			y=rnd(256)-128,
			z=rnd(256)+256+32,
			tx={},ty={}
		}
	end
	grd={} --grid
	for i=0,5 do
		grd[i]={
			x1=-32, x2=32, y1=-32, y2=32,
			z=256/5*i+256+32
		}
	end
	if not debug then music(0) end
end

function _draw()
	cls()
	--------------------draw stars
	foreach(str,
		function(v)
			v.z-=s
			if(v.z<s) then v.z+=256 end
			local x=64+(v.x/v.z)*r
			local y=64+(v.y/v.z)*r
			local c=flr(v.z/32)
			if(x>-m and x<128+m and
			   y>-m and y<128+m) then
				if(v.z<232-s) then
					if(c<2) then
						color(sget(c+6,0))
						line(v.tx[8],v.ty[8],v.tx[6],v.ty[6])
					end
					if(c<4) then
						color(sget(c+4,0))
						line(v.tx[6],v.ty[6],v.tx[4],v.ty[4])
					end
					if(c<6) then
						color(sget(c+2,0))
						line(v.tx[4],v.ty[4],v.tx[2],v.ty[2])
					end
					color(sget(c,0))
					line(v.tx[2],v.ty[2],x,y)
				else
					color(sget(c,0))
					pset(x,y)
				end
			end
			for j=8,2,-1 do
				v.tx[j],v.ty[j] = v.tx[j-1],v.ty[j-1]
			end
			v.tx[1],v.ty[1] = x,y
	 end)
	---------------------draw grid
	foreach(grd,
		function(v)
			local z=0
			v.z-=s
			if(v.z<s) then v.z+=256 end
			local x1=64+(v.x1/v.z)*r
			local x2=64+(v.x2/v.z)*r
			local y1=64+(v.y1/v.z)*r
			local y2=64+(v.y2/v.z)*r
			local c=flr(v.z/32)
			if(y1>-m and y1<128+m and
			   v.z<256-s) then
				color(sget(c,1))
				line(x1,y1,x2,y1)
			end
			if(y2>-m and y2<128+m and
			   v.z<256-s) then
				color(sget(c,1))
				line(x1,y2,x2,y2)
			end
		end)
	--------------------------logo
	local tc=flr(t/4+40)%128
	for j=0,13 do
		local c1=sget(tc+j,2)
		local c2=sget(tc-j,3)
		for i=0,78 do
			local c=sget(i,j+16)
			if(c==3 and c1!=0) then
				pset(24+i,58+j,c1)
			end
			if(c==11 and c2!=0) then
				pset(24+i,58+j,c2)
			end
		end
	end
end

function _update60()
	t+=1
	a+=0.05
	r=128+64*sin(a*ar)
	s=4-3*sin(a*ar)

	--------------------------keys
	local dx = (btn(0) and -1 or 0) + (btn(1) and 1 or 0)
	local dy = (btn(2) and -1 or 0) + (btn(3) and 1 or 0)
	if dx != 0 or dy != 0 then
		foreach(str, function(v) v.x+=dx v.y+=dy end)
		foreach(grd, function(v) v.x1+=dx v.x2+=dx v.y1+=dy v.y2+=dy end)
	end
	if(btnp(4)) then
	end
end

